<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="JavaMixtapeTesting" parallel="tests" thread-count="1">
<parameter name="selenium.browser" value="chrome"/>
<parameter name="signin" value="true"/>
<test name="PlayLists">
    <classes>
        <class name="TestObjects.CSVTests.MadilenaPlaylistChillTunesTest"/>
    </classes>
</test>
<test name="FrontEnd">
    <parameter name="selenium.browser" value="firefox"/>
    <parameter name="signin" value="true"/>
    <classes>
        <class name="TestObjects.CSVTests.LoginPageTests"/>
    </classes>
</test>
</suite>

